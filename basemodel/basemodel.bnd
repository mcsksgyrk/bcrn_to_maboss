Node ULK1 {
  logic = (!mTOR & AMPK);
  rate_up = @logic ? $u_ULK1 : 0;
  rate_down = @logic ? 0 : $d_ULK1;
}

Node mTOR {
  logic = (!ULK1 & !AMPK);
  rate_up = @logic ? $u_mTOR : 0;
  rate_down = @logic ? 0 : $d_mTOR;
}

Node AMPK {
  logic = (!ULK1 & !mTOR);
  rate_up = @logic ? $u_AMPK : 0;
  rate_down = @logic ? 0 : $d_AMPK;
}

